citHeader("To cite package 'occ' in publications use:")

citEntry(
	entry = "Article",
	author = personList(
		as.person("Joaquim Radua"),
		as.person("Santiago Bullich"),
		as.person("Natalia Lopez"),
		as.person("Ana M Catafau")
	),
	title = "Restricted maximum likelihood estimation of PET neuroreceptor occupancy in the absence of a reference region",
	journal = "Medical Physics",
	year = 2011,
	volume = 38,
	pages = 2558,
	textVersion = "Radua J, Bullich S, Lopez N and Catafau AM. Restricted maximum likelihood estimation of PET neuroreceptor occupancy in the absence of a reference region. Medical Physics 2011;38:2558"
)

